{
	"name": "google-maps",
	"description": "Wrapper for asynchronously used Google Maps API",
	"version": "1.0.0",
	"author": {
		"name": "David Kudera",
		"email": "sakren@gmail.com"
	},
	"keywords": [
		"google",
		"maps",
		"asynchronous",
		"browser",
		"wrapper"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:sakren/node-google-maps.git"
	},
	"license": "MIT",
	"engines": {
		"node": "*"
	},
	"main": "./lib/Google.js",
	"devDependencies": {
		"uglify-js": "~2.4.12"
	},
	"scripts": {
		"build": "npm run compile && npm run minify",
		"compile": "coffee -co ./lib/ ./src/",
		"minify": "node ./node_modules/uglify-js/bin/uglifyjs ./lib/Google.js -o ./lib/Google.min.js"
	}
}